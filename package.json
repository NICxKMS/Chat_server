{
  "name": "chat-api",
  "version": "1.0.0",
  "description": "Centralized Chat API for multiple LLM providers with performance optimizations",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --watch src --ext js,json --exec node src/server.js",
    "dev": "nodemon --watch src --ext js,json --exec node src/server.js",
    "dev:ts": "nodemon --watch ts/src --ext ts,json --exec ts-node ts/src/index.ts",
    "build": "mkdir -p dist && cp -r src/protos dist/protos && babel src --out-dir dist --copy-files",
    "buildwin": "mkdir dist && ncp src/protos dist/protos && babel src --out-dir dist --copy-files",
    "build:ts": "tsc --project ts/tsconfig.json",
    "postbuild": "cp -r ./src/protos ./dist/protos",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.ts src/ --fix",
    "load-test": "node scripts/load-test.js",
    "stream-test": "node scripts/stream-test.js",
    "benchmark": "node scripts/benchmark.js",
    "benchmark:doctor": "node scripts/benchmark.js doctor",
    "benchmark:flame": "node scripts/benchmark.js flame",
    "tsc": "tsc --project ts/tsconfig.json",
    "proto:generate": "protoc --plugin=protoc-gen-ts=.\\node_modules\\.bin\\protoc-gen-ts.cmd --ts_out=.\\ts\\src\\types\\proto --proto_path=.\\src\\protos .\\src\\protos\\*.proto",
    "proto:watch": "nodemon --watch src/protos --ext proto --exec npm run proto:generate",
    "start:classifier": "cd src/microservices/model-categorizer/go && go run main.go --log",
    "start:with-classifier": "concurrently \"npm run start:classifier\" \"npm run start\""
  },
  "keywords": [
    "chat",
    "api",
    "openai",
    "anthropic",
    "gemini",
    "openrouter",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@fastify/compress": "^6.0.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/express": "^4.0.2",
    "@fastify/helmet": "^10.0.0",
    "@google/generative-ai": "^0.24.0",
    "@grpc/grpc-js": "^1.13.2",
    "@grpc/proto-loader": "^0.7.13",
    "axios": "^1.8.4",
    "concurrently": "^9.1.2",
    "dotenv": "16.4.7",
    "fastify": "^4.0.0",
    "firebase-admin": "^13.2.0",
    "openai": "^4.90.0",
    "prom-client": "^14.2.0",
    "rate-limiter-flexible": "^6.2.1",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.0",
    "ncp": "^2.0.0",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@eslint/js": "^9.23.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^20.17.28",
    "autocannon": "^7.10.0",
    "chalk": "^5.4.1",
    "clinic": "^12.0.0",
    "eslint": "^9.23.0",
    "grpc-tools": "^1.13.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "protobufjs-cli": "^1.1.3",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.0",
    "typescript": "^5.8.2"
  }
}
